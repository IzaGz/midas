{
  "name": "midas",
  "version": "1.1.2",
  "description": "Syntax highlighter based on PostCSS.",
  "bin": {
    "midas": "bin/cmd.js"
  },
  "main": "dist/index.js",
  "files": [
    "bin",
    "LICENSE-MIT",
    "dist"
  ],
  "scripts": {
    "prepublish": "babel src --out-dir dist --ignore /__tests__/ && node theme.js",
    "test": "babel-tape-runner \"src/**/__tests__/*.js\" | tap-spec"
  },
  "keywords": [
    "css",
    "highlight",
    "postcss"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-tape-runner": "^1.2.0",
    "js-yaml": "^3.4.1",
    "postcss-map": "^0.6.0",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.0"
  },
  "homepage": "https://github.com/ben-eb/midas",
  "author": {
    "name": "Ben Briggs",
    "email": "beneb.info@gmail.com",
    "url": "http://beneb.info"
  },
  "repository": "ben-eb/midas",
  "dependencies": {
    "html-tags": "^1.1.1",
    "minimist": "^1.2.0",
    "object-assign": "^4.0.1",
    "postcss": "^5.0.4",
    "postcss-selector-parser": "^1.2.0",
    "postcss-value-parser": "^2.0.1",
    "read-file-stdin": "^0.2.0",
    "write-file-stdout": "0.0.2"
  }
}
